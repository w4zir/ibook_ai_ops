# Local environment defaults (safe for dev)
ENVIRONMENT=local

# PostgreSQL (Airflow + MLflow metadata)
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_USER=webook
POSTGRES_PASSWORD=webook
POSTGRES_AIRFLOW_DB=airflow
POSTGRES_MLFLOW_DB=mlflow

# Redis (Feast online store)
REDIS_HOST=localhost
REDIS_PORT=6379
# REDIS_PASSWORD=

# MinIO (S3-compatible storage)
MINIO_ENDPOINT=http://localhost:9000
MINIO_ACCESS_KEY=minioadmin
MINIO_SECRET_KEY=minioadmin
MINIO_BUCKET=mlflow

# MLflow
MLFLOW_TRACKING_URI=http://localhost:5000
MLFLOW_ARTIFACT_ROOT=s3://mlflow/

# Airflow
AIRFLOW_WEBSERVER_URL=http://localhost:8080

# Feast (Phase 1 only validates config; definitions come in Phase 2)
FEAST_OFFLINE_STORE=duckdb
FEAST_DUCKDB_PATH=data/processed/feast_offline.duckdb

